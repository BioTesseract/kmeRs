citHeader("To cite kmeRs in publications use:")

citEntry(entry = "Manual",
   title = "kmeRs: an R package for k-mers similarity score calculations",
   author = personList(as.person("Rafal Urniaz")),
   url = "https://cran.r-project.org/package=kmeRs",
   year   = "2024",
   textVersion = "2.1",
   paste("Urniaz RD (2024) kmeRs: an R package for k-mers similarity score calculations. R package version 2.1.0.",
         "https://cran.r-project.org/package=kmeRs")
)
